{
  "manifest_version": 3,
  "name": "Block network requests",
  "description": "Block network requests based in a static hosts ruleset",
  "version": "1.0",
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "block",
        "enabled": true,
        "path": "block.json"
      }
    ]
  },
  "permissions": [
    "declarativeNetRequest"
  ]
}
